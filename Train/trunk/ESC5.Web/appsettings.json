{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AppSetting": {
    "LogRoot": "AppLog"
  },
  "SessionTimeout": 20,
  "CORS": {
    "AllowHeaders": "X-Requested-With,X-XSRF-TOKEN-FORM,X-XSRF-TOKEN-LOGIN,X-CLIENT-TYPE,Content-Type",
    "ExposedHeaders": "X-XSRF-TOKEN-LOGIN,Content-Type"
  }
}
